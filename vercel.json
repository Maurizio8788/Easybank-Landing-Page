{
    "versione": 2 , 
    "public": false , 
    "build": [ 
      { "src": "package.json" , "use": "@ now / static-build" } ,     
      { "src": "index.js" , "use": "@ now / node-server" }     
    ] ,
    "route": [ 
      { "src": "/api/.*" , "dest": "/api/server.js" } ,     
      { "src": "^ / static /(.*)" , "dest": "/ static / $ 1" } ,     
      { "src": ". *" , "dest": "/index.html" }     
    ]
  }
  